{
  "name": "ng-jsonschema",
  "version": "0.0.2",
  "private": false,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run copy && ng build packages",
    "build:fullapp": "npm run build:app && rimraf src/ng-jsonschema",
    "build:lib": "rimraf dist && ng-packagr -p ng-jsonschema/package.json",
    "build:sass": "npm rebuild node-sass",
    "publish-npm": "npm run build:lib && cd dist/ng-jsonschema && npm publish",
    "test": "npm run build:lib && npm run build:fullapp && npm run e2e",
    "copy": "mkdir -p src/ng-jsonschema/ && cp -R dist/ng-jsonschema/* src/ng-jsonschema",
    "testdemo": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4205",
    "kill": "kill $(lsof -t -i:4205)"
  },
  "dependencies": {
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@progress/kendo-file-saver": "^1.0.7",
    "ang-jsoneditor": "^1.9.1",
    "angular2-text-mask": "^8.0.4",
    "core-js": "^2.4.1",
    "js-brasil": "^2.0.7",
    "jsoneditor": "^8.5.3",
    "jszip": "^3.2.2",
    "rxjs": "^6.2.2",
    "text-mask-addons": "^3.7.2",
    "tslib": "^1.9.0",
    "zone.js": "^0.11.3"
    "webpack": "~4.26.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/schematics": "^0.4.0",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "node-sass": "^4.12.0",
    "@types/jasmine": "~2.8.0",
    "@types/node": "~9.4.0",
    "codelyzer": "~4.2.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.4.5",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.1",
    "ts-node": "~5.0.0",
    "tsickle": "^0.27.1",
    "tslint": "~5.9.0",
    "typescript": "3.1.1"
  }
}
