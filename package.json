{
  "name": "ng-jsonschema",
  "version": "0.0.3",
  "private": false,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run copy && ng build packages",
    "build:fullapp": "npm run build:app && rimraf src/ng-jsonschema",
    "build:lib": "rimraf dist && ng-packagr -p ng-jsonschema/package.json",
    "build:sass": "npm rebuild node-sass",
    "publish-npm": "npm run build:lib && cd dist/ng-jsonschema && npm publish",
    "test": "npm run build:lib && npm run build:fullapp && npm run e2e",
    "copy": "mkdir -p src/ng-jsonschema/ && cp -R dist/ng-jsonschema/* src/ng-jsonschema",
    "testdemo": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4205",
    "kill": "kill $(lsof -t -i:4205)"
  },
  "dependencies": {
    "@angular/common": "^11.2.9",
    "@angular/compiler": "^11.2.9",
    "@angular/core": "^11.2.9",
    "@angular/forms": "^11.2.9",
    "@angular/platform-browser": "^11.2.9",
    "@angular/platform-browser-dynamic": "^11.2.9",
    "@angular/router": "^11.2.9",
    "@progress/kendo-file-saver": "^1.0.7",
    "ang-jsoneditor": "^1.9.1",
    "angular2-text-mask": "^8.0.4",
    "core-js": "^2.4.1",
    "js-brasil": "^2.0.7",
    "jsoneditor": "^8.5.3",
    "jszip": "^3.2.2",
    "rxjs": "^6.2.2",
    "text-mask-addons": "^3.7.2",
    "tslib": "^2.0.0",
    "webpack": "~4.46.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.8",
    "@angular-devkit/schematics": "^11.2.8",
    "@angular/cli": "^11.2.8",
    "@angular/compiler-cli": "^11.2.9",
    "@angular/language-service": "^11.2.9",
    "node-sass": "^4.12.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "rimraf": "^2.6.1",
    "ts-node": "~5.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.1.5"
  }
}
